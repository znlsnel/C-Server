<?xml version="1.0" encoding="utf-8" ?>

<PDL>
	<packet name ="S_BroadcastEnterGame">
		<int name ="playerId"/>
		<vector3 name ="position"/>
		<vector3 name ="moveDir"/>
		<vector3 name ="destPoint"/>
	</packet>

	<packet name ="C_Chat">
		<string name ="playerName"/>
		<string name ="ChatText"/>
	</packet>
 
	<packet name ="C_AttackRequset">
		<int name ="playerId"/>
	</packet>
	
	<packet name ="S_BroadcastAttackRequset">
		<int name ="playerId"/>
	</packet>
		 
	<packet name ="C_Damage">
		<int name ="attackedPlayerId"/>
		<int name ="damagedPlayerId"/>
	</packet>
		
	<packet name ="S_BroadcastDamage">
		<int name ="attackedPlayerId"/>
		<int name ="damagedPlayerId"/>
	</packet>
	
	<packet name ="S_BroadcastScoreUpdate">
		<int name ="team1Score"/>
		<int name ="team2Score"/> 
		<int name ="team3Score"/>
	</packet>
	
		
	<packet name ="S_BroadcastChat">
		<string name ="playerName"/>
		<string name ="ChatText"/>
	</packet>

	<packet name="C_LeaveGame">
	</packet>
	<packet name="S_BroadcastLeaveGame">
		<int name ="playerId"/>
	</packet>
	<packet name ="S_PlayerList">
		<list name ="player">  
			<bool name ="isSelf"/> 
			<int name ="playerId"/>
			<vector3 name ="position"/>
			<vector3 name ="moveDir"/>
			<vector3 name ="destPoint"/>
		</list> 
	</packet>
	<packet name ="C_Move">
		<vector3 name ="position"/>
		<vector3 name ="moveDir"/>
		<vector3 name ="destPoint"/>
	</packet>
	<packet name ="S_BroadcastMove">
		<int name ="playerId"/>
		<vector3 name ="position"/>
		<vector3 name ="moveDir"/>
		<vector3 name ="destPoint"/>
	</packet>
</PDL>
